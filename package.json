{
  "name": "nodejs-training",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/salsita/nodejs-training"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "postinstall": "npm run migrate up && npm run snyk protect",
    "start": "node ./src/index.mj",
    "lint": "eslint -c .eslintrc.js .",
    "lintfix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "migrate": "node-pg-migrate",
    "snyk": "snyk",
    "apidoc": "apidoc -f \".*\\.js$\" -i src/actions/ -o apidoc/"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "chai": "4.1.2",
    "cross-env": "5.1.3",
    "eslint": "4.18.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-security": "1.4.0",
    "mocha": "5.0.1",
    "sinon": "4.3.0",
    "sinon-chai": "2.14.0",
    "supertest": "3.0.0"
  },
  "dependencies": {
    "dotenv": "5.0.0",
    "http-status": "1.0.1",
    "joi": "13.1.2",
    "jsonwebtoken": "8.1.1",
    "koa": "2.5.0",
    "koa-bodyparser": "4.2.0",
    "koa-compress": "2.0.0",
    "koa-cors": "0.0.16",
    "koa-helmet": "3.3.0",
    "koa-morgan": "1.0.1",
    "koa-router": "7.4.0",
    "koa-send": "4.1.3",
    "koa-static-server": "1.3.3",
    "lodash": "4.17.5",
    "moment": "2.20.1",
    "node-pg-migrate": "2.23.0",
    "pg": "7.4.1",
    "snyk": "1.69.7",
    "squel": "5.12.0",
    "winston": "2.4.0"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "snyk": true
}
