{
  "name": "nodejs-training",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/salsita/nodejs-training"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "npm run snyk protect",
    "build:client": "npm install --prefix client-auth",
    "start": "node ./src",
    "start:dev": "nodemon ./src",
    "lint": "eslint -c eslintrc.js .",
    "lintfix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "migrate": "node-pg-migrate",
    "snyk": "snyk",
    "apidoc": "apidoc -f \".*\\.js$\" -i src/actions/ -o apidoc/",
    "heroku-postbuild": "npm run migrate up"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "1.1.0",
    "koa-send": "5.0.0",
    "koa-static": "5.0.0",
    "lint-staged": "7.3.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "puppeteer": "1.8.0",
    "sinon": "6.3.4",
    "sinon-chai": "3.2.0",
    "supertest": "3.3.0"
  },
  "dependencies": {
    "@salsita/errors": "1.0.1",
    "@salsita/koa-error-middleware": "1.1.0",
    "@salsita/koa-joi-middleware": "1.0.1",
    "@salsita/koa-jwt-auth": "1.0.1",
    "@salsita/koa-server": "1.3.0",
    "@salsita/prune-values": "1.0.0",
    "@salsita/psql": "1.0.1",
    "dotenv": "6.0.0",
    "http-status": "1.2.0",
    "is-wsl": "1.1.0",
    "joi": "13.7.0",
    "jsonwebtoken": "8.3.0",
    "koa-passport": "4.1.1",
    "koa-router": "7.4.0",
    "lodash": "4.17.11",
    "node-pg-migrate": "3.11.0",
    "passport-github": "1.1.0",
    "passport-google-oauth": "1.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.4.3",
    "pretty-error": "2.1.1",
    "snyk": "1.100.0",
    "squel": "5.12.2",
    "winston": "3.1.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run lintfix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "10.8.0",
    "npm": "6.3.0"
  },
  "snyk": true
}
