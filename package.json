{
  "name": "nodejs-training",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/salsita/nodejs-training"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "postinstall": "npm run snyk protect",
    "build:client": "npm install --prefix client-auth",
    "start": "node ./src",
    "start:dev": "nodemon ./src",
    "lint": "eslint -c eslintrc.js .",
    "lintfix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "migrate": "node-pg-migrate",
    "snyk": "snyk",
    "apidoc": "apidoc -f \".*\\.js$\" -i src/actions/ -o apidoc/"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "chai": "4.1.2",
    "cross-env": "5.1.6",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "0.14.3",
    "koa-send": "4.1.3",
    "koa-static": "4.0.3",
    "lint-staged": "7.1.3",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "prettier": "1.13.4",
    "puppeteer": "1.5.0",
    "sinon": "5.1.0",
    "sinon-chai": "3.1.0",
    "supertest": "3.1.0"
  },
  "dependencies": {
    "@salsita/errors": "1.0.0",
    "@salsita/koa-error-middleware": "1.0.0",
    "@salsita/koa-joi-middleware": "1.0.0",
    "@salsita/koa-jwt-auth": "1.0.0",
    "@salsita/koa-server": "1.1.0",
    "@salsita/prune-values": "1.0.0",
    "@salsita/psql": "1.0.0",
    "dotenv": "6.0.0",
    "http-status": "1.1.2",
    "is-wsl": "1.1.0",
    "joi": "13.4.0",
    "jsonwebtoken": "8.2.2",
    "koa-passport": "4.1.0",
    "koa-router": "7.4.0",
    "lodash": "4.17.10",
    "node-pg-migrate": "3.5.0",
    "passport-github": "1.1.0",
    "passport-google-oauth": "1.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.4.3",
    "snyk": "1.82.1",
    "squel": "5.12.1",
    "winston": "2.4.2"
  },
  "lint-staged": {
    "*.js": [
      "npm run lintfix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "10.2.1",
    "npm": "6.1.0"
  },
  "snyk": true
}
