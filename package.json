{
  "name": "nodejs-training",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/salsita/nodejs-training"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "npm run build:client && npm run snyk protect",
    "build:client": "SKIP_PREFLIGHT_CHECK=true npm ci --prefix client-auth",
    "postoutdated": "npm outdated --prefix client-auth",
    "start": "node ./src",
    "start:dev": "nodemon ./src",
    "lint": "eslint -c eslintrc.js .",
    "lintfix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha --recursive --require ./mocha.bootstrap.js \"./{,!(node_modules|client-auth)/**/}*.spec.js\"",
    "posttest": "SKIP_PREFLIGHT_CHECK=true npm test --prefix client-auth",
    "test:watch": "npm test -- --watch",
    "migrate": "node-pg-migrate",
    "snyk": "snyk",
    "apidoc": "apidoc -f \".*\\.js$\" -i src/actions/ -o apidoc/",
    "heroku-postbuild": "npm run migrate up -- --no-reject-unauthorized"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "apidoc": "0.25.0",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-security": "1.4.0",
    "husky": "4.2.5",
    "koa-send": "5.0.1",
    "koa-static": "5.0.0",
    "lint-staged": "10.3.0",
    "mocha": "8.1.3",
    "nodemon": "2.0.4",
    "prettier": "2.1.1",
    "puppeteer": "5.2.1",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@koa/router": "9.4.0",
    "@salsita/errors": "1.0.2",
    "@salsita/koa-error-middleware": "1.1.1",
    "@salsita/koa-joi-middleware": "1.0.3",
    "@salsita/koa-jwt-auth": "1.0.2",
    "@salsita/koa-server": "1.3.1",
    "@salsita/prune-values": "1.0.1",
    "@salsita/psql": "1.0.2",
    "dotenv": "8.2.0",
    "http-status": "1.4.2",
    "is-wsl": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "koa-passport": "4.1.3",
    "lodash": "4.17.20",
    "node-pg-migrate": "5.6.0",
    "passport-github": "1.1.0",
    "passport-google-oauth": "2.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.3.3",
    "pretty-error": "2.1.1",
    "snyk": "1.389.0",
    "squel": "5.13.0",
    "winston": "3.3.3"
  },
  "lint-staged": {
    "*.js": [
      "npm run lintfix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.8"
  },
  "snyk": true
}
